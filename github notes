ssh://github.com/dumpholster/dumpholster.github.io.git
https://github.com/rishified/rishified.github.io.git
https://github.com/war-damn/war-damn.github.io.git

git clone git@github-dumpholser:ssh://github.com/dumpholster/dumpholster.github.io.git dumpholster.com
git remote add origin git@github-example:example/Test.git

git clone ssh://git@github-dumpholser/dumpholster/dumpholster.github.io.git ~/git/dumpholster.github.io



. ~/.bash_profile

git commit --amend --reset-author



git clone https://github.com/dumpholster/dumpholster.github.io.git ~/git/dumpholster/dumpholster.github.io
git clone https://github.com/rishified/rishified.github.io.git ~/git/rishified/rishified.github.io
git clone https://github.com/war-damn/war-damn.github.io.git ~/git/war-damn/war-damn.github.io

git config user.name dumpholster; git config user.email brandon@auburn.edu
git config user.name rishified; git config user.email mahtani@auburn.edu
git config user.name war-damn; git config user.email brandon@war-damn.com



mkdir ~/git/dumpholster
cd ~/git/dumpholster
git init
git config user.name dumpholster; git config user.email brandon@auburn.edu
git clone https://github.com/dumpholster/dumpholster.github.io.git
cd dumpholster.github.io.git
git remote add origin dumpholster:dumpholster/dumpholster.github.io.git 
git commit --amend --reset-author
git add .
git commit -m "anything"

mkdir ~/git/rishified
cd ~/git/rishified
git init
git config user.name rishified; git config user.email mahtani@auburn.edu
git clone https://github.com/rishified/rishified.github.io.git
cd rishified.github.io.git
git remote add origin rishified:rishified/rishified.github.io.git
git commit --amend --reset-author
git add .
git commit -m "anything"

mkdir ~/git/war-damn
cd ~/git/war-damn
git init
git config user.name war-damn; git config user.email brandon@war-damn.com
git clone https://github.com/war-damn/war-damn.github.io.git
cd war-damn.github.io.git
git remote add origin war-damn:war-damn/war-damn.github.io.git 
git add .
git commit -m "anything"


alias git-clone-dumpholster 'git clone https://github.com/dumpholster/dumpholster.github.io.git ~/git/dumpholster/dumpholster.github.io'
alias git-clone-rishified 'git clone https://github.com/rishified/rishified.github.io.git ~/git/rishified/rishified.github.io'
alias git-clone-war-damn 'git clone https://github.com/war-damn/war-damn.github.io.git ~/git/war-damn/war-damn.github.io'